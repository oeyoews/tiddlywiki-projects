{
  "name": "skill-badges",
  "version": "1.1.1",
  "homepage": "https://github.com/oeyoews/tiddlywiki-projects",
  "description": "",
  "main": "dist/index.js",
  "tsup": {
    "entry": [
      "index.ts"
    ],
    "splitting": false,
    "sourcemap": false,
    "minify": true,
    "dts": true,
    "format": [
      "esm",
      "cjs",
      "iife"
    ],
    "shims": true,
    "clean": true
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ncc build ./index.ts -w -o dist/",
    "nccbuild": "ncc build ./index.ts -o ./dist/ --minify --no-cache",
    "build": "tsup",
    "prepublish": "npm run build --dry",
    "sync": "cnpm sync",
    "preview": "ts-node ./app/index.ts"
  },
  "keywords": [
    "icon",
    "typescript",
    "skill",
    "badge",
    "shields"
  ],
  "author": "oeyoews",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^20.10.5",
    "chalk": "^4.1.2",
    "cnpm": "^9.3.2",
    "ejs": "^3.1.9",
    "html-prettify": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}