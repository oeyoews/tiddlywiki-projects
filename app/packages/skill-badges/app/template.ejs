<h2>Icons</h2>

<table>
	<thead align="center">
		<tr>
			<th>Name</th>
			<th>Icon</th>
		</tr>
	</thead>
	<tbody align="left">
		<% for (const key in badges) { %>
			<tr>
				<th>
					<%- key %>
				</th>
				<th align="center">
					<img src="<%- badges[key] %>" />
				</th>
			</tr>
			<% } %>
	</tbody>
</table>