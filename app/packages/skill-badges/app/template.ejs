<h2>Usage</h2>

<pre><code>
const badges = require('skill-badges').default;
console.log(badges.JavaScript);
</code></pre>

<h2>Icons</h2>

<table>
	<thead align="center">
		<tr>
			<th>Name</th>
			<th>Icon</th>
		</tr>
	</thead>
	<tbody align="center">
		<% for (const key in badges) { %>
			<tr>
				<th>
					<%- key %>
				</th>
				<th align="center">
					<img src="<%- badges[key] %>" />
				</th>
			</tr>
			<% } %>
	</tbody>
</table>