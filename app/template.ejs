<!-- automatically generated at <%= date %> -->
<h2>💼 Tiddlywiki 项目</h2>

<table>
	<thead align="center">
		<tr>
			<th>项目</th>
			<th>简述</th>
			<th>技术</th>
			<th>成就</th>
		</tr>
	</thead>

	<tbody align="left">
		<% projects.forEach(function(project){ %>
			<tr>
				<th>
					<a href="<%= project.github %>" target="_blank">
						<%= project.repo %>
					</a>
				</th>
				<th>
					<%= project.logo %>
						<%= project.desc %>
				</th>
				<th align="center">
					<% project.techList.forEach(function(tech){ %>
						<img src="<%- tech %>" alt="badge">
						<% }); %>
				</th>
				<th align="center">
					<% project.badges.forEach(function(badge){ %>
						<img src="<%- badge %>" alt="badge">
						<% }); %>
				</th>
			</tr>
			<% }); %>
	</tbody>
</table>

<h2>🧩 Tiddlywiki 插件</h2>

<table>
	<thead align="center">
		<tr>
			<th>插件</th>
			<th>简述</th>
			<th>类别</th>
			<th>在线地址</th>
		</tr>
	</thead>

	<tbody align="left">
		<% plugins.forEach(function(plugin){ %>
			<tr>
				<th>
					<a href="<%= plugin.github %>" target="_blank">
						<%= plugin.name %>
					</a>
				</th>
				<th>
					<%= plugin.logo %>
						<%= plugin.desc %>
				</th>
				<th align="center">
					<%= plugin.category %>
				</th>
				<th align="center">
					<a href="<%= plugin.online %>" target="_blank">Online</a>
				</th>
			</tr>
			<% }); %>
	</tbody>
</table>